import java.util.*;
class Solution {
	void sortTheArray (int[] A) {
	 int n=A.length;
		int l=0,mid=0;
		int temp=0;
		int r=n-1;
		while(mid<=r){
			switch(A[mid]){
					case 0:{
						temp=A[l];
						A[l]=A[mid];
						A[mid]=temp;
						l++; mid++; 
						break;
					}
				case 1:
					mid++;
					break;
				case 2:{
					temp=A[mid];
					A[mid]=A[r];
					A[r]=temp;
					r--; 
					break;
				}
			}
		
			//
				
		
			
		}
		
	}
}
